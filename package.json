{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "check": "biome check",
    "check:write": "biome check --fix --unsafe",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev .vercel/output/static",
    "deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "build-cf-types": "wrangler types --env-interface CloudflareEnv env.d.ts"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "next": "15.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/next-on-pages": "^1.13.12",
    "@cloudflare/workers-types": "^4.20250619.0",
    "@types/node": "^20.19.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.0.1",
    "knip": "^5.61.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wrangler": "^4.20.3"
  }
}
